#include "lists.h"

/**
 * add_node_end -  function that adds a new node at the end of a list_t list.
 *
 * @head: pointer to a pointer to the head of the list
 * @str: new string in the node
 *
 * Return: Always 0.
 */

void free_listint(listint_t *head)
{
	if (!head)
		return;

	if (head->next)
	{
		free_listint(head->next);
	}
	free(head->n);
	free(head);
}
