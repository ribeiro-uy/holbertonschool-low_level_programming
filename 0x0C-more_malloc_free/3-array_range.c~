#include "holberton.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * array_range - function that creates an array of integers.
 *
 * @min: min integer
 * @max: max integer
 *
 * Return: Always 0.
 */

int *array_range(int min, int max)
{
	char *array;
	int i;

	array = malloc(max * min);

	if (array == NULL)
	{
		free(array);
	}

	for (min; min <= max; min++)
	{
		array[min] = min;
	}

	return (array);
}
